{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "_h5ai"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/_h5ai/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/_h5ai/index.html"
    }
  ]
}
